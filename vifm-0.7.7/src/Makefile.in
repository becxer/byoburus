update_compile_info:
	@before="$$(cat "compile_info.c" 2> /dev/null)"; \
	if [ "x$$(which git 2> /dev/null)" != "x" ]; then \
		unset GIT_DIR; \
		new="const char GIT_HASH[] = \"$$(git rev-parse HEAD)\";"; \
	else \
		new='const char GIT_HASH[] = "";'; \
	fi; \
	if [ "x$$before" != "x$$new" -o ! -f "compile_info.c" ]; then \
		echo "$$new" > compile_info.c; \
	fi

